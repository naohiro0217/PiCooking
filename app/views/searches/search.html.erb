<% if @model == 'customer' %>
	<h3 class = 'ml-5 mt-3'>"<%= @content %>"さんの検索結果</h3>
	<% if admin_signed_in? %>
	    <%= render 'admin/customers/index', customers: @records %>
	<% elsif customer_signed_in? %>
	    <%= render 'public/customers/index', customers: @records %>
	<% end %>
  <%#= render 'public/customers/index', customers: @records || 'admin/customers/index', customers: @records %>
<% elsif @model == 'cook' || @model == 'tag' || @model == 'rate' %>
	<h3>"<%= @content %>"の検索結果</h3>
  <%#= render 'public/cooks/index', cooks: @records || 'admin/cooks/index', cooks: @records %>
  <% if admin_signed_in? %>
      <%= render 'admin/cooks/index', cooks: @records %>
  <% elsif customer_signed_in? %>
      <%= render 'public/cooks/index', cooks: @records %>
  <% end %>
<% end %>